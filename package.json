{
  "name": "tentix-monorepo",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "start": "bun server/index.ts",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "clean": "rm -rf ./**/node_modules && rm -rf ./**/dist"
  },
  "workspaces": [
    "packages/*",
    "frontend",
    "server"
  ],
  "devDependencies": {
    "drizzle-orm": "0.41.0",
    "@types/node": "^20",
    "tentix-ui": "workspace:*",
    "i18n": "workspace:*",
    "@types/bun": "^1.2.12",
    "@workspace/eslint-config": "workspace:*",
    "@workspace/typescript-config": "workspace:*",
    "prettier": "^3.5.1",
    "tentix-server": "workspace:*",
    "turbo": "^2.5.1",
    "typescript": "5.8.3",
    "@tanstack/react-query": "^5.74.4"
  },
  "packageManager": "bun@1.2.12",
  "engines": {
    "node": ">=20"
  },
  "dependencies": {
    "ahooks": "^3.8.4",
    "lucide-react": "^0.510.0",
    "zustand": "^5.0.3"
  }
}
